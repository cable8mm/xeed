{
    "name": "cable8mm/xeed",
    "description": "The Xeed is to generate new model, seed, database seed, factory and migration files for Laravel based on data from the existing database table.",
    "keywords": [
        "laravel",
        "factory",
        "seed",
        "migration",
        "generator",
        "xeed"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "vlucas/phpdotenv": "^5.0",
        "cable8mm/array-flatten": "^1.0",
        "symfony/console": "^6.0|^7.0",
        "doctrine/inflector": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Cable8mm\\Xeed\\": "src/",
            "Cable8mm\\Xeed\\Database\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\Xeed\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/pint",
        "inspect": "./vendor/bin/pint --test",
        "apidoc": "rm -rf builder; doctum.phar update doctum.php --output-format=github --no-ansi --no-progress;"
    }
}